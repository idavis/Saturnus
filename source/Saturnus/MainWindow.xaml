<Window x:Class="Saturnus.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Saturnus="clr-namespace:Saturnus" xmlns:Views="clr-namespace:Saturnus.Views" x:Name="Window"
        Title="MainWindow" Height="411" Width="600">
    <Grid DataContext="{Binding ViewModel, ElementName=Window}">
        <Views:ShellView DataContext="{Binding ShellViewModel}" Height="Auto" Width="Auto" />
    </Grid>
</Window>
